{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es5",
    "sourceMap": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "esModuleInterop": true,
    "lib": [
      "dom",
      "es5",
      "es2015",
      "es2016",
      "es2017",
      "es2018"
    ]
  }
}
