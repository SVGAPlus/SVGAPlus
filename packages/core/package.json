{
  "name": "@svgaplus/core",
  "version": "1.0.0",
  "description": "Enhanced SVGA Player.",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm test && npm run build",
    "dev": "webpack-dev-server --config ./build/webpack.dev.js",
    "build:normal": "webpack --config ./build/webpack.build.js",
    "build:worker": "cross-env IS_WORKER_BUILD=true webpack --config ./build/webpack.build.js",
    "build": "npm run build:normal & npm run build:worker",
    "pb": "npx pbf ./src/proto/svga.proto > ./src/core/proto/svga.proto.js",
    "test": "jest"
  },
  "keywords": [
    "SVGA",
    "SVGAPlayer",
    "SVGAPlus"
  ],
  "files": [
    "dist/",
    "types/",
    "index.d.ts",
    "README.md"
  ],
  "author": {
    "name": "LancerComet",
    "email": "chw644@hotmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@types/jest": "25.2.1",
    "babel-loader": "8.0.6",
    "canvas": "2.6.1",
    "cross-env": "6.0.3",
    "css-loader": "2.1.1",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "25.4.0",
    "jest-github-actions-reporter": "^1.0.2",
    "pbf": "^3.2.1",
    "pixi.js": "^5.3.3",
    "terser-webpack-plugin": "^4.1.0",
    "ts-jest": "25.4.0",
    "ts-loader": "5.4.3",
    "tslint": "5.20.1",
    "typescript": "3.7.4",
    "webpack": "4.41.4",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1",
    "worker-loader": "2.0.0"
  }
}
